type Listing {
  id: ID!
  title: String
}

type LisitngsConnection {
  edges: [ListingsEdge!]!
  pageInfo: PageInfo!
}

type ListingsEdge {
  cursor: ID!
  node: Listing
}

type PageInfo {
  startCursor: ID!
  endCursor: ID!
  hasNextPage: Boolean
}

type Srp {
  id: ID!
  name: String
  lisitngsConnection(first: Int = 20, after: ID): LisitngsConnection
}

type Query {
  getSrp(id: ID!): Srp!
}
